{
  "name": "ai-qa-engineer-town",
  "version": "1.0.0",
  "description": "AI QA Engineer Assistant",
  "private": true,
  "workspaces": [
    "frontend",
    "functions"
  ],
  "scripts": {
    "build": "npm run build --workspace=frontend",
    "frontend": "npm run dev --workspace=frontend",
    "functions": "npm run build --workspace=functions",
    "functions:watch": "npm run build -- --watch --preserveWatchOutput --workspace=functions",
    "dev": "npm run frontend",
    "install:all": "npm install && npm install --workspace=frontend && npm install --workspace=functions"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/genai": "^0.14.1",
    "@google/generative-ai": "^0.24.1"
  }
}
